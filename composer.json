{
  "name"        : "src-run/augustus-utility-library",
  "license"     : "MIT",
  "type"        : "library",
  "homepage"    : "https://src.run/augustus-utility-library",
  "description" : "collection of utility objects for strings, arrays, classes, etc",
  "support" : {
    "email"  : "rmf@src.run",
    "source" : "https://src.run/augustus-utility-library/git",
    "issues" : "https://src.run/augustus-utility-library/issues"
  },
  "keywords" : [
    "augustus-utility-library", "augustus", "utility", "library", "rmf", "src-run"
  ],
  "authors" : [
    {
      "name"     : "Rob Frawley 2nd",
      "email"    : "rmf@src.run",
      "homepage" : "https://src.run/rmf",
      "role"     : "Project Lead"
    }
  ],
  "require" : {
    "php" : "^5.6||^7.0"
  },
  "require-dev" : {
    "phpunit/phpunit"        : "^5.0",
    "codacy/coverage"        : "^1.0",
    "satooshi/php-coveralls" : "^1.0"
  },
  "autoload" : {
    "psr-4" : {
      "SR\\Utility\\" : "lib/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "SR\\Utility\\Tests\\" : "tests/"
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true,
  "config" : {
    "bin-dir" : "bin"
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "0.6-dev"
    }
  }
}
