{
    "name"        : "src-run/augustus-utility-library",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://src.run/augustus-utility-library",
    "description" : "This library contains a core collection of generic type utilities (strings, classes, etc).",
    "support" : {
        "email"  : "rmf@src.run",
        "issues" : "https://augustus-utility-library.oss.src.run/issues",
        "source" : "https://augustus-utility-library.oss.src.run/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "augustus-utility-library", "reflection", "utilities", "library", "rmf", "src-run", "scribe"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@src.run",
            "homepage": "https://src.run/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php" : "^5.6|^7.0"
    },
    "require-dev" : {
        "phpunit/phpunit"        : "^5.0",
        "codacy/coverage"        : "^1.0",
        "satooshi/php-coveralls" : "^1.0"
    },
    "autoload" : {
        "psr-4" : {
            "SR\\Utility\\" : "lib/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "SR\\Utility\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "0.6-dev"
        }
    }
}
