{
  "name": "src-run/augustus-utility-library",
  "license": "MIT",
  "type": "library",
  "homepage": "https://src.run/augustus-utility-library",
  "description": "A simple collection of utilities for strings, arrays, objects, buffering, and more",
  "support": {
    "email": "rmf@src.run",
    "source": "https://src.run/augustus-utility-library/git",
    "issues": "https://src.run/augustus-utility-library/issues"
  },
  "keywords": [ "augustus-utility-library", "augustus", "utility", "library", "rmf", "src-run" ],
  "authors": [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "require": {
    "php": "~7.1",
    "src-run/augustus-silencer-library": "~0.8"
  },
  "require-dev": {
    "codacy/coverage": "~1.0",
    "friendsofphp/php-cs-fixer": "~2.0",
    "phpunit/phpunit": "~5.7",
    "satooshi/php-coveralls": "~2.0",
    "symfony/phpunit-bridge": "~3.0 | ~4.0",
    "symfony/var-dumper": "~3.4 | ~4.0"
  },
  "autoload": {
    "psr-4": {
      "SR\\Interpreter\\": "lib/Interpreter/",
      "SR\\Output\\Buffered\\": "lib/Output/Buffered/",
      "SR\\Utilities\\": "lib/Utilities/",
      "SR\\Util\\": ".lib-bc/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SR\\Interpreter\\Test\\": "tests/Interpreter/",
      "SR\\Output\\Buffered\\Test\\": "tests/Output/Buffered/",
      "SR\\Utilities\\Test\\": "tests/Utilities/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.8-dev"
    }
  }
}
