{
  "name": "src-run/augustus-utility-library",
  "license": "MIT",
  "type": "library",
  "homepage": "https://src.run/augustus-utility-library",
  "description": "A simple collection of utilities for strings, arrays, objects, buffering, and more",
  "support": {
    "email": "rmf@src.run",
    "source": "https://src.run/augustus-utility-library/git",
    "issues": "https://src.run/augustus-utility-library/issues"
  },
  "keywords": [ "augustus-utility-library", "augustus", "utility", "library", "rmf", "src-run" ],
  "authors": [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "require": {
    "php": "^8.0",
    "src-run/augustus-silencer-library": "^0.10.0"
  },
  "require-dev": {
    "codacy/coverage": "^1.4",
    "friendsofphp/php-cs-fixer": "^2.0",
    "php-coveralls/php-coveralls": "^2.4",
    "symfony/phpunit-bridge": "^4.0 | ^5.0",
    "symfony/var-dumper": "^4.0 | ^5.0"
  },
  "autoload": {
    "psr-4": {
      "SR\\Utilities\\": "lib/"
    },
    "files": [
      "stub/class-alias-stub.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SR\\Tests\\Utilities\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.10-dev"
    }
  }
}
